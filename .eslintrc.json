{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/prefer-default-export": 0,
        "import/no-extraneous-dependencies": 0,
        "import/extensions": 0,
        "import/no-unresolved": 0,
        "no-plusplus": 0,
        "max-len": 0,
        "no-mixed-operators": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "no-empty": 0,
        "no-shadow": 0,
        "no-bitwise": 0
    }
}
